{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "node": true
    },
    "globals": {
        "describe": true,
        "it": true,
        "beforeEach": true,
        "before": true,
        "after": true,
        "afterEach": true
    },
    "extends": "eslint:recommended"
}
